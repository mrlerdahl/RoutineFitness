@model LiftsViewModel

@{
    ViewData["Title"] = "LiftDetail";
    Layout = "~/Views/Shared/_LayoutTwo.cshtml";
}


    <div style="text-align: center">
        <h2>LiftDetail</h2>
        @foreach (var l in Model.Lifts)
        {
            <video width="300" height="350" controls>
                <source src="@l.VideoUrl" type="video/mp4">
            </video>

            <p>@l.LiftName</p>
            <p>@l.LiftDescription</p>
            <a asp-action="AddToList" asp-controller="CustomWorkout" class="btn btn-primary" asp-route-liftName="@l.LiftName" asp-route-liftId="@l.LiftId">Add Lift</a>
        }

    </div>



